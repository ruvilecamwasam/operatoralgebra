   RowBox[{
    RowBox[{
     OverscriptBox["a", "^"], "\[SmallCircle]", 
     OverscriptBox["b", "^"]}], "=", 
    OverscriptBox["c", "^"]}], TraditionalForm]],ExpressionUUID->
  "3dcec957-6baa-49c0-8e75-d76f90cc4d9e"],
 " using the notation: contraction[",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["a", "^"], ",", 
    OverscriptBox["b", "^"], ",", 
    OverscriptBox["c", "^"]}], TraditionalForm]],ExpressionUUID->
  "0babda04-f4af-40c8-bfab-9caf54ca0be3"],
