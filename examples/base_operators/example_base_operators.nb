 CellChangeTimes->{{3.934165093034872*^9, 3.9341651254011917`*^9}},
 CellLabel->
  "In[386]:=",ExpressionUUID->"70a68b4a-1e8c-3140-83ba-338d474cb699"],

Cell[BoxData[
 RowBox[{
  FractionBox[
   RowBox[{
    OverscriptBox["x", "^"], "\[SmallCircle]", 
    OverscriptBox["p", "^"], "\[SmallCircle]", 
    OverscriptBox["p", "^"], "\[SmallCircle]", 
    OverscriptBox["p", "^"]}], 
   RowBox[{"2", " ", "m"}]], "+", 
  RowBox[{
   FractionBox["1", "2"], " ", "m", " ", 
