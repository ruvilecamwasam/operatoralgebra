Cell[CellGroupData[{

Cell["Example: The harmonic oscillator", "Section",
 CellChangeTimes->{{3.9341637315716324`*^9, 
  3.934163736430294*^9}},ExpressionUUID->"e39cdfde-0033-cb40-ba9a-\
11780d3c7475"],

Cell["Let\[CloseCurlyQuote]s keep the same commutators we defined earlier:", \
"Text",
 CellChangeTimes->{{3.934165154160187*^9, 
  3.9341651641846657`*^9}},ExpressionUUID->"f7c5ff66-6efa-b94e-81ec-\
2a63258dc46f"],

Cell[BoxData[{
 RowBox[{"Commutator", "[", 
  RowBox[{
   OverscriptBox["a", "^"], ",", 
   OverscriptBox["ad", "^"], ",", "1"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"Commutator", "[", 
  RowBox[{
   OverscriptBox["p", "^"], ",", 
   OverscriptBox["x", "^"], ",", 
   RowBox[{
    RowBox[{"-", "\[ImaginaryI]"}], " ", "\[HBar]"}]}], "]"}]}], "Input",
 CellChangeTimes->{{3.9341651656707497`*^9, 3.9341651661005096`*^9}},
 CellLabel->
  "In[387]:=",ExpressionUUID->"e4c52b79-58ab-9f44-9afe-bebe7590d50e"],

Cell[CellGroupData[{

Cell["Defining the Hamiltonian", "Subsection",
 CellChangeTimes->{{3.9341652465351734`*^9, 
  3.934165249650957*^9}},ExpressionUUID->"96d7b799-b900-d546-8cec-\
c0d288361b8b"],

Cell["\<\
In the position basis, the  Harmonic oscillator has Hamiltonian\
\>", "Text",
 CellChangeTimes->{{3.934164857512224*^9, 3.934164862281044*^9}, {
  3.9341651472268124`*^9, 
  3.934165171366314*^9}},ExpressionUUID->"0b5e7a09-7e1e-ba48-87a3-\
4e2e4f319745"],

Cell[BoxData[
 RowBox[{
  RowBox[{"H", "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", "2"], "m", " ", 
     SuperscriptBox["\[Omega]", "2"], 
     RowBox[{
      OverscriptBox["x", "^"], "\[SmallCircle]", 
      OverscriptBox["x", "^"]}]}], "+", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", "m"}]], 
     RowBox[{
      OverscriptBox["p", "^"], "\[SmallCircle]", 
      OverscriptBox["p", "^"]}]}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.934164863221344*^9, 3.9341648825467205`*^9}},
 CellLabel->
  "In[376]:=",ExpressionUUID->"d6593419-efa0-e244-8299-58f21d2f6ad3"],

Cell[TextData[{
 "Note that above, we did not declare ",
 StyleBox["H",
  FontSlant->"Italic"],
 " as an operator. This is because whenever Mathematica sees ",
 StyleBox["H",
  FontSlant->"Italic"],
 ", it will be replaced by the right-hand side, which are all operators. \n\n\
No harm would be done if you did declare H as an operator:"
}], "Text",
 CellChangeTimes->{{3.9341648841417713`*^9, 3.93416509109474*^9}, {
  3.9341653081536446`*^9, 
  3.93416531301132*^9}},ExpressionUUID->"0279ccd6-e13b-1c47-80bc-\
c42e6c0a3993"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   OverscriptBox["H2", "^"], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", "2"], "m", " ", 
     SuperscriptBox["\[Omega]", "2"], 
     RowBox[{
      OverscriptBox["x", "^"], "\[SmallCircle]", 
      OverscriptBox["x", "^"]}]}], "+", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", "m"}]], 
     RowBox[{
      OverscriptBox["p", "^"], "\[SmallCircle]", 
      OverscriptBox["p", "^"]}]}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.9341650399304714`*^9, 3.93416506758547*^9}},
 CellLabel->
  "In[382]:=",ExpressionUUID->"83f952b0-ce8e-9a44-8fe3-761e0af97b91"],

Cell["We can see that both definitions will work:", "Text",
 CellChangeTimes->{{3.9341650754919643`*^9, 
  3.9341651127702675`*^9}},ExpressionUUID->"f330f66f-43c5-274e-87b6-\
4b29e4dcf4bc"],

Cell[CellGroupData[{

Cell[BoxData["H"], "Input",
 CellChangeTimes->{{3.934165049698452*^9, 3.9341650730290375`*^9}},
 CellLabel->
  "In[383]:=",ExpressionUUID->"51210662-d8ae-8e41-83aa-256a6ff823fe"],

Cell[BoxData[
 RowBox[{
  FractionBox[
   RowBox[{
    OverscriptBox["p", "^"], "\[SmallCircle]", 
    OverscriptBox["p", "^"]}], 
   RowBox[{"2", " ", "m"}]], "+", 
  RowBox[{
   FractionBox["1", "2"], " ", "m", " ", 
   SuperscriptBox["\[Omega]", "2"], " ", 
   RowBox[{
    OverscriptBox["x", "^"], "\[SmallCircle]", 
    OverscriptBox["x", "^"]}]}]}]], "Output",
 CellChangeTimes->{3.934165073693981*^9},
