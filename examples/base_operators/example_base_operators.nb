Cell[TextData[{
 "Let\[CloseCurlyQuote]s define a substitution following the ladder operation \
definition (",
 ButtonBox["see Wikipedia",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://en.wikipedia.org/wiki/Quantum_harmonic_oscillator#Ladder_\
operator_method"], None},
  ButtonNote->
   "https://en.wikipedia.org/wiki/Quantum_harmonic_oscillator#Ladder_operator_\
method"],
 "):"
}], "Text",
 CellChangeTimes->{{3.934165324960867*^9, 3.934165397386881*^9}, {
  3.934165803176279*^9, 
  3.9341658057444916`*^9}},ExpressionUUID->"9a73aeaa-af6e-6446-bd6f-\
59d7116e5705"],

Cell[BoxData[
 RowBox[{
  RowBox[{"opSub", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      OverscriptBox["x", "^"], "->", 
      RowBox[{
       SqrtBox[
        FractionBox["\[HBar]", 
         RowBox[{"2", " ", "m", " ", "\[Omega]"}]]], 
       RowBox[{"(", 
        RowBox[{
         OverscriptBox["ad", "^"], "+", 
         OverscriptBox["a", "^"]}], ")"}]}]}], ",", 
     RowBox[{
      OverscriptBox["p", "^"], "->", 
      RowBox[{"\[ImaginaryI]", 
       SqrtBox[
        FractionBox[
         RowBox[{"\[HBar]", " ", "m", " ", "\[Omega]"}], "2"]], 
       RowBox[{"(", 
        RowBox[{
         OverscriptBox["ad", "^"], "-", 
         OverscriptBox["a", "^"]}], ")"}]}]}]}], "}"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.934165372776934*^9, 3.934165424746748*^9}, {
  3.93416547525832*^9, 3.934165478178266*^9}},
 CellLabel->
  "In[143]:=",ExpressionUUID->"35a2f2a7-0523-834d-91fe-1211977cfbd0"],

Cell[TextData[{
 "Note that you can enter division using ",
 StyleBox["Ctrl + /",
  FontSlant->"Italic"],
 ", square roots using ",
 StyleBox["Ctrl+2",
  FontSlant->"Italic"],
 ", and ",
 "\[Omega] using ",
 StyleBox["[Esc]w[Esc]",
  FontSlant->"Italic"],
 ".\n",
 "\nWe can see that this transformation diagonalises the Hamiltonian:"
}], "Text",
 CellChangeTimes->{{3.93416542657827*^9, 3.9341654332462816`*^9}, {
