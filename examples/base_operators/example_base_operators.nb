  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", "4"}], "-", 
     RowBox[{"\[ImaginaryI]", " ", "\[Alpha]"}], "+", "\[Gamma]"}], ")"}], 
   " ", 
   RowBox[{
    OverscriptBox["x", "^"], "\[SmallCircle]", 
