Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"-", "\[ImaginaryI]"}], " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", "4"}], "+", "\[Gamma]"}], ")"}], " ", "\[HBar]"}], "+", 
  RowBox[{"2", " ", 
   RowBox[{
    OverscriptBox["p", "^"], "\[SmallCircle]", 
